{
  "baseUrl": "",
  "titlePrefix": "TutorTrack",
  "titleSuffix": "",
  "favicon": "images/github-icon.png",
  "style": {
    "bootstrapTheme": "none"
  },
  "globalOverride": {
    "header": "_markbind/navigation.md"
  },
  "head": "<link rel=\"stylesheet\" href=\"{{baseUrl}}/stylesheets/main.css\">",
  "pages": [
    {
      "src": "index.md",
      "title": "TutorTrack"
    },
    {
      "src": "UserGuide.md",
      "title": "User Guide"
    },
    {
      "src": "DeveloperGuide.md",
      "title": "Developer Guide"
    },
    {
      "src": "AboutUs.md",
      "title": "About Us"
    },
    {
      "src": "Configuration.md",
      "title": "Configuration Guide"
    },
    {
      "src": "Documentation.md",
      "title": "Documentation Guide"
    },
    {
      "src": "Testing.md",
      "title": "Testing Guide"
    },
    {
      "src": "Logging.md",
      "title": "Logging Guide"
    },
    {
      "src": "SettingUp.md",
      "title": "Setting Up"
    },
    {
      "src": "DevOps.md",
      "title": "DevOps Guide"
    },
    {
      "glob": "team/*.md"
    }
  ],
  "ignore": [
    "_site/*",
    "*.json",
    "*.log",
    ".sass-cache/*",
    "_includes/*",
    "_layouts/*",
    "_sass/*",
    "_config.yml",
    "Gemfile*",
    "_data/*"
  ],
  "plugins": [
    "filterTags"
  ],
  "pluginsContext": {
    "filterTags": {
      "tags": ["preview"]
    }
  },
  "headingIndexingLevel": 4
}
